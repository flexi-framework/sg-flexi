module-whatis	"Enable usage of FFTW FIGIVERSION with GNU compilers"

conflict fftw

# FFTW variables
set FFTW_LEVEL FIGIVERSION
set FFTW_CURPATH  FIGIPATH
setenv FFTW_DIR $FFTW_CURPATH
setenv FFTW_INCLUDE_OPTS $FFTW_CURPATH/include
prepend-path LD_LIBRARY_PATH $FFTW_CURPATH/lib
prepend-path PATH $FFTW_CURPATH/bin

proc ModulesHelp {} {
  puts stderr "
modulefile \"[module-info name]\"
Enable usage of FFTW FIGIVERSION"
  return 0
}
